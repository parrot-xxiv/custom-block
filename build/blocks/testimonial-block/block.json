{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "custom-block/testimonial-block",
  "version": "0.1.0",
  "title": "Testimonial",
  "category": "my-custom-category",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "testimonialText": {
      "type": "string",
      "source": "html",
      "selector": ".testimonial-text",
      "default": "default"
    }
  },
  "textdomain": "testimonial-block",
  "editorScript": "file:./index.js",
  "style": "file:../tailwind.css"
}