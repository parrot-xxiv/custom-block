{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "custom-block/parallax-banner",
	"version": "0.1.0",
	"title": "Parallax Banner",
	"category": "my-custom-category",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"parallaxMediaId": {
			"type": "number"
		},
		"parallaxMediaUrl": {
			"type": "string",
			"source": "attribute",
			"selector": ".banner-bg img",
			"attribute": "src"
		}
	},
	"textdomain": "parallax-banner",
	"editorScript": "file:./index.js",
	"style": "file:../tailwind.css"
}